{
  "commands": {
    "unix_commands": [
      "ls -la",
      "grep -r 'pattern' .",
      "find . -name '*.js' -type f",
      "cat file.txt | grep 'search'",
      "ps aux | grep 'node'",
      "mkdir -p nested/dir/structure",
      "cp -r source/ destination/",
      "mv oldfile.txt newfile.txt",
      "curl -X POST https://api.example.com/data",
      "tail -f logfile.txt",
      "head -n 10 data.txt",
      "wc -l *.txt",
      "chmod +x script.sh",
      "tar -xzf archive.tar.gz",
      "ln -s target linkname"
    ],
    "powershell_expected": [
      "Get-ChildItem -la",
      "Get-ChildItem -Recurse | Select-String -Pattern 'pattern'",
      "Get-ChildItem -Path . -Filter '*.js' -Recurse",
      "Get-Content file.txt | Select-String -Pattern 'search'",
      "Get-Process | Select-String -Pattern 'node'",
      "New-Item -ItemType Directory -Path nested\\dir\\structure -Force",
      "Copy-Item -Path source\\ -Destination destination\\ -Recurse",
      "Move-Item -Path oldfile.txt -Destination newfile.txt",
      "Invoke-RestMethod -Uri https://api.example.com/data -Method POST",
      "Get-Content logfile.txt -Wait",
      "Get-Content data.txt -Head 10",
      "Get-ChildItem *.txt | ForEach-Object { (Get-Content $_.FullName | Measure-Object -Line).Lines }",
      "# Windows equivalent: icacls or Set-ExecutionPolicy",
      "Expand-Archive -Path archive.tar.gz -DestinationPath .",
      "New-Item -ItemType SymbolicLink -Path linkname -Target target"
    ],
    "complex_pipelines": [
      "ps aux | grep node | awk '{print $2}' | xargs kill",
      "find . -name '*.log' -exec grep -l 'ERROR' {} \\;",
      "cat access.log | grep '404' | cut -d' ' -f1 | sort | uniq -c",
      "ls -la | grep '^d' | wc -l"
    ]
  },
