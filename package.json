{
  "name": "claude-flow-windows",
  "version": "1.0.0-alpha.5",
  "description": "Windows-compatible MCP server for Claude swarm coordination and agent orchestration",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "claude-flow": "dist/cli.js",
    "claude-flow-windows": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "test": "vitest",
    "prepublishOnly": "npm run build",
    "mcp:serve": "node dist/index.js"
  },
  "keywords": [
    "mcp",
    "claude",
    "swarm",
    "orchestration",
    "windows",
    "powershell",
    "agent",
    "coordination",
    "ai-agents",
    "cognitive-diversity",
    "model-context-protocol"
  ],
  "author": "Jordan Ehrig <jordan@ehrig.dev>",
  "license": "MIT",
  "homepage": "https://github.com/jordanehrig/claude-flow-windows#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jordanehrig/claude-flow-windows.git"
  },
  "bugs": {
    "url": "https://github.com/jordanehrig/claude-flow-windows/issues"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.63.0",
    "@modelcontextprotocol/sdk": "^0.5.0",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.11.0",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "cross-spawn": "^7.0.3",
    "dotenv": "^16.3.1",
    "node-ipc": "^10.1.0",
    "uuid": "^9.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.25",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "os": [
    "win32",
    "darwin",
    "linux"
  ],
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
